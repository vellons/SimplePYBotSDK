{
  "id": "webots_khr2hv_v1",
  "version": "0.1",
  "name": "Example robot configuration for Webots",
  "motion_file": "./example_webots_khr2hv_motion.json",
  "enable_twist_controller": true,
  "enable_parser_json_commands": true,
  "motors": {
    "head_z": {
      "id": "Head",
      "offset": 0.0,
      "type": "virtual-servo",
      "angle_limit": [
        -75,
        75
      ],
      "orientation": "direct"
    },
    "l_shoulder_x": {
      "id": "Left_Shoulder",
      "offset": 0.0,
      "type": "virtual-servo",
      "angle_limit": [
        -90,
        90
      ],
      "orientation": "direct"
    },
    "r_shoulder_x": {
      "id": "Right_Shoulder",
      "offset": 0.0,
      "type": "virtual-servo",
      "angle_limit": [
        -90,
        90
      ],
      "orientation": "indirect"
    },
    "l_shoulder_y": {
      "id": "Left_Arm1",
      "offset": 0.0,
      "type": "virtual-servo",
      "angle_limit": [
        0,
        180
      ],
      "orientation": "direct"
    },
    "r_shoulder_y": {
      "id": "Right_Arm1",
      "offset": 0.0,
      "type": "virtual-servo",
      "angle_limit": [
        0,
        180
      ],
      "orientation": "indirect"
    },
    "l_elbow_y": {
      "id": "Left_Hand",
      "offset": 0.0,
      "type": "virtual-servo",
      "angle_limit": [
        -125,
        125
      ],
      "orientation": "direct"
    },
    "r_elbow_y": {
      "id": "Right_Hand",
      "offset": 0.0,
      "type": "virtual-servo",
      "angle_limit": [
        -125,
        125
      ],
      "orientation": "indirect"
    }
  },
  "motors_type": {
    "virtual-servo": {
      "angle_speed": 60
    }
  },
  "sensors": {
    "gyroscope_x": {
      "id": "gyroscope_x",
      "offset": 0.0
    },
    "gyroscope_y": {
      "id": "gyroscope_y",
      "offset": 0.0
    },
    "gyroscope_z": {
      "id": "gyroscope_z",
      "offset": 0.0
    }
  },
  "poses": {
    "standby": {
      "head_z": 0,
      "l_shoulder_x": 0,
      "r_shoulder_x": 0,
      "l_shoulder_y": 0,
      "r_shoulder_y": 0,
      "l_elbow_y": 0,
      "r_elbow_y": 0
    },
    "t": {
      "head_z": 0,
      "l_shoulder_x": -90,
      "r_shoulder_x": -90,
      "l_shoulder_y": 90,
      "r_shoulder_y": 90,
      "l_elbow_y": 0,
      "r_elbow_y": 0
    }
  }
}
